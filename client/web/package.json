  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "graphql:type": "npm run graphql:download && npm run graphql:generate",
    "graphql:download": "yarn run apollo schema:download --endpoint=http://b23ce7eaab80.ngrok.io/graphql graphql-schema.json",
    "graphql:generate": "yarn run apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --includes=src/**/*.ts --tagName=gql --addTypename --excludes=src/GraphQL/common/**/*.ts --globalTypesFile=src/types/graphql-global-types.ts types",
    "generate:code": "graphql-codegen --config codegen.yml"
  },
  "eslintConfig": {
    "extends": [
