{
  "kind": "collectionType",
  "collectionName": "items",
  "info": {
    "name": "Items"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "label": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "LANGUAGE",
        "SKILLS",
        "PROJECT_TYPES"
      ]
    },
    "languages": {
      "via": "language",
      "collection": "languages"
    },
    "profiles": {
      "via": "job_seniority_id",
      "collection": "profiles"
    },
    "profile_skills": {
      "via": "skill_id",
      "collection": "profile-skills"
    },
    "profile_project_types": {
      "via": "project_type_id",
      "collection": "profile-project-types"
    }
  }
}
